  {
    "name": "postedDate",
    "type": "com.linkedin.common.Date",
    "doc": "The date on which the article was posted.",
    "optional": true
  },
  {
    "name": "numLikes",
    "type": "int",
    "doc": "The number of likes that this post has.",
    "optional": true
  },
  {
    "name": "numViews",
    "type": "int",
    "doc": "The number of views that this post has.",
    "optional": true
  },
  {
    "name": "numComments",
    "type": "int",
